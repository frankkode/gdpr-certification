{
     "version": 2,
     "functions": {
       "server.js": {
         "runtime": "nodejs20.x"
       }
     },
     "routes": [
       {
         "src": "/health",
         "dest": "/server.js"
       },
       {
         "src": "/stats", 
         "dest": "/server.js"
       },
       {
         "src": "/generate",
         "dest": "/server.js"
       },
       {
         "src": "/verify/(.*)",
         "dest": "/server.js"
       },
       {
         "src": "/(.*)",
         "dest": "/server.js"
       }
     ]
   }